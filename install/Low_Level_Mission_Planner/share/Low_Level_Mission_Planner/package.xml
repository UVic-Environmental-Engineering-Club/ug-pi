<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>Low_Level_Mission_Planner</name>
  <version>0.0.2</version>
  <description>Package for reading and publishing data received from the onboard sensors.</description>
  <maintainer email="daehwankim@uvic.ca">David Kim</maintainer>
  <maintainer email="penguinmillion@gmail.com">Anthony Cieri</maintainer>
  <license>MIT</license>

  <depend>rclpy</depend>
  <depend>std_msgs</depend>
  <depend>ms5837_sensor</depend>
  <depend>smbus2</depend>
  <depend>sys</depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
